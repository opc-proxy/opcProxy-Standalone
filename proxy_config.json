       "RESTConnector":    true,
       "PromConnector" : true,
       
       "gRPC" :{
	       "port" : 5051
       },
	"influx":
	{
		"organizationName" : "pollo",
	        "bucketName" : "paul"
	},
       
	"RESTapi": {
	       "serverLog":true,
               "port" : 8087,
               "urlPrefix" : "",
	       "host":"*",
               "enableCookieAuth" : true,
	       "staticFilesPath" : "./public/",
	       "enableStaticFiles": true,
	       "MJEmail": "xenoscope@physik.uzh.ch"
       },

       "prometheus":{
		"variableFilter" : {
			"matchRegEx" : ["^"],
			"notContain": ["Req","SP"]
		}
       }
