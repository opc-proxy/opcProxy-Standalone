{
    "opcServerURL":"opc.tcp://localhost:4840/freeopcua/server/",

    "loggerConfig" :{
        "loglevel" :"info"
    },

    "nodesLoader" : {
        "targetIdentifier" : "browseName",
	"contains":["PV","MyVar"]

    },
    "nodesDatabase" :{
	    "isInMemory":true
    },
       "grpcConnector" :   false,
       "influxConnector" : false,
